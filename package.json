{
  "name": "my-next-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -r \"npx nexus dev\" \"npx next\" \"npx graphql-codegen --watch\" \"npm run watch:prisma\"",
    "build": "next build",
    "start": "next start",
    "watch:prisma": "onchange prisma/**/*.prisma -- prisma generate",
    "type-check": "tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "test": "jest"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.1",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@prisma/client": "^2.14.0",
    "@zeit/next-source-maps": "0.0.3",
    "apollo-server-micro": "^2.19.1",
    "framer-motion": "^3.1.4",
    "graphql-tag": "^2.11.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.4",
    "next-urql": "^2.1.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.27.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "urql": "^1.11.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typed-document-node": "^1.18.2",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@prisma/cli": "^2.14.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
